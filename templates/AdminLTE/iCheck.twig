{% block stylesheets_page %}
    {{ assets.css('css/iCheck') | raw }}
{% endblock %}

{% block scripts_page %}

    <!-- Include page-specific JS -->
    {{ assets.js('js/iCheck') | raw }}

    <script>
        $(function() {
            $('#{{box_id}}').iCheck({
              checkboxClass: 'icheckbox_square-blue',
              radioClass: 'iradio_square-blue',
              increaseArea: '20%' // optional
            });
        });
    </script>
{% endblock %}